{
  "publisher": "nyxb",
  "name": "vscode-nyxi",
  "displayName": "vscode-nyxi",
  "version": "0.0.2",
  "description": "ðŸ”§ Using @nyxb/nyxi via vscode extension.",
  "author": "Dennis Ollhoff <contact@nyxb.xyz>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nyxb/vscode-nyxi.git"
  },
  "bugs": {
    "url": "https://github.com/nyxb/vscode-nyxi/issues"
  },
  "keywords": [
    "package-manager",
    "npm",
    "yarn",
    "pnpm",
    "vscode",
    "vscode-extension"
  ],
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "icon": "logo.png",
  "files": [],
  "engines": {
    "vscode": "^1.79.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "nyxi",
        "title": "Nyxi"
      },
      {
        "command": "nyxi.nyxi",
        "title": "Install [nyxi]"
      },
      {
        "command": "nyxi.nyxr",
        "title": "Run [nyxr]"
      },
      {
        "command": "nyxi.nyxlx",
        "title": "Execute [nyxlx]"
      },
      {
        "command": "nyxi.nyxu",
        "title": "Upgrade [nyxu]"
      },
      {
        "command": "nyxi.nyxun",
        "title": "Uninstall [nyxun]"
      },
      {
        "command": "nyxi.nyxci",
        "title": "Clean Install [nyxci]"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "nyxi",
          "group": "nyxi"
        },
        {
          "submenu": "nyxi.editor.context",
          "group": "nyxi"
        }
      ],
      "nyxi.editor.context": [
        {
          "command": "nyxi.nyxi",
          "group": "nyxi"
        },
        {
          "command": "nyxi.nyxr",
          "group": "nyxi"
        },
        {
          "command": "nyxi.nyxlx",
          "group": "nyxi"
        },
        {
          "command": "nyxi.nyxu",
          "group": "nyxi"
        },
        {
          "command": "nyxi.nyxun",
          "group": "nyxi"
        },
        {
          "command": "nyxi.nyxci",
          "group": "nyxi"
        }
      ]
    },
    "submenus": [
      {
        "id": "nyxi.editor.context",
        "label": "Nyxi Commands"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "nyxr build",
    "build": "tsup",
    "watch": "tsup --watch",
    "test": "nyxr build && node ./node_modules/vscode/bin/test",
    "release": "nyxlx changelogen@latest --release && vsce publish --no-dependencies"
  },
  "devDependencies": {
    "@nyxb/eslint-config": "^0.0.53",
    "@nyxb/nyxi": "^0.0.30",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.5",
    "@types/vscode": "^1.79.0",
    "execa": "^7.1.1",
    "nyxi-for-vscode": "^0.0.3",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3"
  }
}
